    <footer class="mt-4 bg-main-color pt-1 pb-1">
        <div class="container">
            <p>
                <table style="width: 100%; table-layout: fixed;">
                    <tr>
                        <td>
                            {% if facebook_page is not empty %}
                                <a href="{{ facebook_page }}"><img src="{{ asset("bundles/app/img/facebook.ico") }}"/></a>
                            {% endif %}
                            {% if youtube_page is not empty %}
                                <a href="{{ youtube_page }}"><img src="{{ asset("bundles/app/img/youtube.ico") }}"/></a>
                            {% endif %}
                            {% if diaspora_page is not empty %}
                                <a href="{{ diaspora_page }}"><img src="{{ asset("bundles/app/img/diaspora.png") }}"/></a>
                            {% endif %}
                            {% if instagram_url is not empty %}
                                <a href="{{ instagram_url }}"><img src="{{ asset("bundles/app/img/instagram.png") }}"/></a>
                            {% endif %}
                        </td>
                        {% if newsletter_url is not empty %}
                            <td style="text-align: center;">
                                <a class="btn btn-sm btn-outline-white" style="color: white;" onmouseout="this.style.color = 'white';" onmouseover="this.style.color = 'var(--main-color)';" data-toggle="modal" data-target="#modal_newsletter">{{ "subscribe"|trans }}</a>
                            </td>
                        {% endif %}
                        <td style="text-align: right;">
                            {% if mainAssociation is defined and mainAssociation != null %}
                                <a class="btn btn-sm btn-outline-white" style="color: white;" onmouseout="this.style.color = 'white';" onmouseover="this.style.color = 'var(--main-color)';" href="{{ url("app_association", {"id": mainAssociation.id, "name": mainAssociation.nameForUrl}) }}">{{ "who_are_we"|trans }}</a>
                            {% endif %}
                            <a class="btn btn-sm btn-outline-white" style="color: white;" onmouseout="this.style.color = 'white';" onmouseover="this.style.color = 'var(--main-color)';" href="{{ path("app_legal_terms") }}">{{ "about"|trans }}</a>
                        </td>
                    </tr>
                </table>
            </p>
        </div>
    </footer>


    {% if newsletter_url is not empty %}
        <div class="modal fade" id="modal_newsletter" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-body">
                        <iframe class="mj-w-res-iframe"
                                frameborder="0"
                                scrolling="no"
                                marginheight="0"
                                marginwidth="0"
                                src="{{ newsletter_url }}"
                                width="100%"></iframe>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary" data-dismiss="modal">{{ "close"|trans }}</button>
                    </div>
                </div>
            </div>
        </div>

        <script type="text/javascript" src="https://app.mailjet.com/statics/js/iframeResizer.min.js"></script>
    {% endif %}
